{{#each this}}
<div class="photo-card">
    <div class="photo-card__image">
        <a href={{largeImageURL}}>
            <img src={{webformatURL}} alt="" loading="lazy" />
        </a>
    </div>
  <div class="info">
    <p class="info-item">
      <b>Likes</b> {{likes}}
    </p>
    <p class="info-item">
      <b>Views</b> {{views}}
    </p>
    <p class="info-item">
      <b>Comments</b> {{comments}}
    </p>
    <p class="info-item">
      <b>Downloads</b> {{downloads}}
    </p>
  </div>
</div>
{{/each}}